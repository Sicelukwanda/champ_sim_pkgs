	</joint>

	<link name="left_swivel_wheel">
		<inertial>
			<mass value="0.1"/>	
			<origin xyz="0 0 0"/>
			<inertia ixx="0.012411765597" ixy="-0.000711733678" ixz="0.00050272983"
				 iyy="0.015218160428" iyz="-0.000004273467" izz="0.011763977943"/>
  	 	</inertial>
	<visual name="left_swivel_wheel">
		<origin xyz="0 0 0" rpy="0 0 0"/>
		<geometry name="swivel_wheel">
			<mesh filename="${mesh_directory}/center_wheel.stl" />
		</geometry>
		<material name="WheelBlack">
			<color rgba="0.117 0.117 0.117 1"/>
		</material>
	</visual>
	<collision>
		<origin xyz="0 0 0" rpy=" ${-3.1415927/2} 0 0"/>
		<geometry>
			<cylinder radius="0.0669" length="0.0422"/>
		</geometry>
	</collision>
	</link>
	<gazebo reference="swivel_wheel">
		<material value="Gazebo/Black"/>
	</gazebo>

	<joint name="left_swivel_wheel_hubcap_joint" type="fixed">
		<origin xyz="0 0 0" rpy="0 0 0"/>
		<parent link="left_swivel_hubcap"/>
		<child link="left_swivel_wheel"/>
	</joint>
	
	<!--Left Wheel and hub-->


	<link name="left_hubcap">
	     <inertial>
		<mass value="0.08"/>  
		<origin xyz="0 0 0"/>
		<inertia ixx="0.012411765597" ixy="0" ixz="0"
		         iyy="0.015218160428" iyz="0" izz="0.011763977943"/>
	      </inertial>
		<visual>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<geometry>
				<mesh filename="${mesh_directory}/hubcap.stl"/>
			</geometry>
			<material name="HubcapWhite">
				<color rgba="1.0 1.0 1.0 1.0"/>
			</material>
		</visual>
	      <collision>
		<origin xyz="0 0 0" rpy="0 0 0"/>
		<geometry>
		  <mesh filename="${mesh_directory}/hubcap.stl"/>
		</geometry>
	      </collision>
	</link>

	<joint name="left_hub_joint" type="continuous">
		<origin xyz="0.1 0.35 -0.0835" rpy="0 0 0"/>
	        <anchor xyz="0 0 0"/>
	        <limit effort="100" velocity="100" />
	        <joint_properties damping="0.0" friction="0.0" />
		<parent link="base_center"/>
		<child link="left_hubcap"/>
		<axis xyz="0 1 0"/>
	</joint>

	<link name="left_wheel">
	      <inertial>
		<mass value="1.25"/> 
		<origin xyz="0 0 0"/>
		<inertia ixx="0.012411765597" ixy="0" ixz="0"
		         iyy="0.015218160428" iyz="0" izz="0.011763977943"/>
	      </inertial>
		<visual>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<geometry>
				<mesh filename="${mesh_directory}/wheel.stl" />
			</geometry>
			<material name="WheelBlack">
				<color rgba="0.117 0.117 0.117 1"/>
			</material>
		</visual>
	      <collision>
		<origin xyz="0 0 0" rpy="${-3.1415927/2} 0 0"/>
		<geometry>
		  <cylinder radius="0.1315" length="0.065"/>
		</geometry>
	      </collision>
	</link>

	<joint name="left_wheel_joint" type="fixed">
		<origin xyz="0 0 0" rpy="0 0 0"/>
		<parent link="left_hubcap"/>
		<child link="left_wheel"/>
	</joint>
	
	<gazebo reference="left_wheel">
		<material value="Gazebo/Black"/>
	</gazebo>

	<!--Right Wheel and hub-->


	<link name="right_hubcap">
	     <inertial>
		<mass value="0.08"/>  
		<origin xyz="0 0 0"/>
		<inertia ixx="0.012411765597" ixy="0" ixz="0"
		         iyy="0.015218160428" iyz="0" izz="0.011763977943"/>
	      </inertial>
		<visual>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<geometry>
				<mesh filename="${mesh_directory}/hubcap.stl"/>
			</geometry>
			<material name="HubcapWhite">
