<?xml version="1.0"?>

<robot name="wam_powerbot"
	xmlns:sensor="http://playerstage.sourceforge.net/gazebo/xmlschema/#sensor"
	xmlns:controller="http://playerstage.sourceforge.net/gazebo/xmlschema/#controller"
	xmlns:interface="http://playerstage.sourceforge.net/gazebo/xmlschema/#interface"
	xmlns:xacro="http://ros.org/wiki/xacro">


  <xacro:include filename="$(find urdf_pkg)/defs/powerbot_wheel.xacro"/>

  <!-- Chassis -->
  <link name="base_center">
    <inertial>
      <mass value="40.5"/>
      <!--<origin xyz="-0.025 0 -0.223"/>-->
      <origin xyz="-0.05 0 0"/>
      <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
    </inertial>
    <visual name="base_visual">
      <origin rpy="0 0 0" xyz="-0.045 0 0.148"/>
      <geometry name="pioneer_geom">
        <mesh filename="package://urdf_pkg/meshes/p3dx_meshes/chassis.stl"/>
      </geometry>
      <material name="ChassisYellow">
        <color rgba="0.715 0.583 0.210 1.0"/>
      </material>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="-0.045 0 0.145"/>
      <geometry>
        <box size="4.05 0.25 0.14"/>
      </geometry>
    </collision>
  </link>
  <gazebo reference="base_center">
    <material value="Gazebo/Red"/>
  </gazebo>
  <!-- Top -->
  <joint name="base_top_joint" type="fixed">
    <origin rpy="0 0 0" xyz="-0.045 0 0.234"/>
    <parent link="base_center"/>
    <child link="top_plate"/>
  </joint>
  <link name="top_plate">
    <inertial>
      <mass value="0.1"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
    </inertial>
    <visual name="base_visual">
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry name="top_geom">
        <mesh filename="package://urdf_pkg/meshes/p3dx_meshes/top.stl"/>
      </geometry>
      <material name="TopBlack">
        <color rgba="0.038 0.038 0.038 1.0"/>
      </material>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0.0 0 0"/>
      <geometry name="pioneer_geom">
        <box size="5.05 0.38 0.01"/>
      </geometry>
    </collision>
  </link>
  <gazebo reference="top_plate">
    <material value="Gazebo/Black"/>
  </gazebo>
	
	
<!-- ArmStuff -->

<link name="arm_plate">
	<inertial>
		<mass value="0.01"/>	
		<origin xyz="0 0 0"/>
		<inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
	</inertial>
	<visual>			
		<geometry>
			<box size="0.45 0.75 0.05"/>
		</geometry>
		<origin xyz="0.2 0 0.03" rpy="0 0 0"/>
		<material name="brown">
			<color rgba="0.8 0.4 0.3 1"/>
		</material>
	</visual>
</link>

<joint name="arm_plate_joint" type="fixed">
	<parent link="top_plate"/>
	<child link="arm_plate"/>
	<origin xyz="0 0 0"/>
</joint>

	<link name="wam_link_base">
		<inertial>
			<mass value="20"/>
			<origin xyz="0.0 0.0 0.0"/>
			<inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
