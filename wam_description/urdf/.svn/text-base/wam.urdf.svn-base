		<visual>
			<origin rpy="0 0 0" xyz="0 0 0"/>
			<geometry>
				<mesh filename="package://wam_description/mesh/wambase.stl" scale="1.0 1.0 1.0"/>
			</geometry>
			<material name="Grey">
				<color rgba="0.75 0.75 0.75 1.0"/>
			</material>
		</visual>
		<collision>
			<origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
